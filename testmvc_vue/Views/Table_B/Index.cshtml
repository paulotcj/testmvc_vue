@model IEnumerable<testmvc_vue.Models.Table_B>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Col1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Col2)
            </th>
